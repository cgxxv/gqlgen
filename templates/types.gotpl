{{ define "objectNode" }}
type _{{ .Type.GraphQLName}}Node struct {
	_fields []collectedField

    {{range $field := .Fields -}}
        {{ if $field.Type.Scalar -}}
            {{$field.GraphQLName|ucFirst}} {{ $field.Type.Local }}
        {{ else -}}
            {{$field.GraphQLName|ucFirst}} {{if $field.Type.IsSlice}}[]{{end}}Writer
        {{ end -}}
    {{end}}
}
{{ end }}
